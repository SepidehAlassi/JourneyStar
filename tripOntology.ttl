@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix currencyA: <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/> .
@prefix currency: <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/ISO4217-CurrencyCodes/>.
@prefix schema: <https://schema.org/> .
@prefix : <http://www.mysemantics.com/ontology/trip/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@base <http://www.mysemantics.com/> .

<http://www.mysemantics.com/ontology/hp/> rdf:type owl:Ontology ;
                                       rdfs:comment "A trip ontology." ;
                                       owl:versionInfo "v.1.0" .



################################
### Classes ###
################################

### Person ###
:Person rdf:type owl:Class ;
    rdfs:subClassOf schema:Person;
    rdfs:label "Person"@de ,
                "Person"@en ;
    rdfs:comment "Represents a Person."@en .

### Location ###
:Location rdf:type owl:Class ;
        rdfs:subclassOf dbr:Location; #schema:Place dbo:place dbo:populatedArea
        rdfs:label  "Ort"@de,
                    "Location"@en;
        rdfs:comment "A class defining a location"@en .


## add location nature? desert>> vulcano >> glacier 

### Trip ###
:Trip rdf:type owl:Class ;
        rdfs:label "Reisen"@de ,
                "Trip"@en ;
        rdfs:comment "Any type of movement between two locations."@en .

### Journey  ### 
:Journey rdf:type owl:Class ;
        rdfs:subClassOf :Trip;
        owl:differentFrom :Excursion ;
        rdfs:label "Reise"@de ,
                "Journey"@en ;
        rdfs:comment "A journey from one place to another."@en .

### Excursion ###
:Excursion rdf:type owl:Class ;
        rdfs:subClassOf :Trip ;
        owl:differentFrom :Journey;
        rdfs:label "Excursion"@de ,
                "Exkursion"@en ;
        rdfs:comment "Represents an excursion during a stay to a location and back to the location of departure."@en .

### Stay ###
:Stay rdf:type owl:Class ;
        rdfs:label "Aufenthalt"@de,
                    "Stay"@en;
        rdfs:comment "A stay during a journey of undetermined amount of time"@en .

### Accommodation ###
:Accommodation rdf:type owl:Class ;
        #rdfs:subClassIf dbo:Building, dbo:Shelter; 
        rdfs:label "Unterkunft"@de ,
                   "Accommodation"@en ;
        rdfs:comment "A resource representing any form of accommodation."@en .

### Activity ###
:Activity rdf:type owl:Class ;
        rdfs:label "Aktivität"@de,
                    "Activity"@en;
        rdfs:comment "A resource describing an activity that happened during a trip or stay."@en .

# SightSeeing #
:SightSeeing rdf:type owl:Class;
        rdfs:subClassOf :Activity;
        rdfs:label "Sehenswürdigkeit"@de,
                    "sightseeing"@en;
        rdfs:comment "A resource describing the sightseeing of a landmark, attraction etc. as an activity."@en .


# Entertainment #
:Entertainment rdf:type owl:Class;
        rdfs:subClassOf :Activity;
        rdfs:label "Unterhaltung"@de,
                    "entertainement"@en;
        rdfs:comment "A resource describing an entertainment activity happening during a trip or stay."@en .

# Visit #
:Visit rdf:type owl:Class;
        rdfs:subClassOf :Activity;
        rdfs:label "Aktivität"@de,
                    "Activity"@en;
        rdfs:comment "A resource describing a visit of a person during a trip or stay "@en .


# Dining #
:Dining rdf:type owl:Class;
        rdfs:subClassOf :Activity;
        rdfs:label "Essen"@de,
                    "dinning"@en;
        rdfs:comment "A resource describing dining as an activity during a trip or stay."@en .



### MeanOfTransportation ###
:ModeOfTransport rdf:type owl:Class ;
        rdfs:subClassOf dbr:Mode_of_transport;
        rdfs:label "Transportmittel"@de,
                    "mode of transportation"@en;
        rdfs:comment "A resource describing a mean of transportation of a trip"@en .
      


