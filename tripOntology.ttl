@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix currency: <https://spec.edmcouncil.org/fibo/ontology/FND/Accounting/CurrencyAmount/> .
@prefix schema: <https://schema.org/> .
@prefix : <http://www.mysemantics.com/ontology/trip/> .
@base <http://www.mysemantics.com/> .

<http://www.mysemantics.com/ontology/hp/> rdf:type owl:Ontology ;
                                       rdfs:comment "A trip ontology." ;
                                       owl:versionInfo "v.1.0" .


################################
## Classes
################################


### Person ###

:Person rdf:type owl:Class ;
    rdfs:subClassOf schema:Person;
    rdfs:label "Person"@de ,
                "Person"@en ;
    rdfs:comment "Represents a Person."@en .

### Location ###
:Location rdf:type owl:Class ;
        rdfs:subclassOf dbo:Location;
        rdfs:label  "Ort"@de,
                    "Location"@en;
        rdfs:comment "A class defining a location"@en .

### Landscape ###
:Landscape rdf:type owl:Class ;
        rdfs:subClassOf :Location ; #schemalandscap
        rdfs:label "Landschaft"@de,
                    "Landscape"@en;
        rdfs:comment "Nature of any type"@en .


### Settlement ###
:Settlement rdf:type owl:Class ;
        rdfs:subclassOf dbo:Settlement, :Location;
        rdfs:label  "Niederlassung/Siedlung"@de,
                    "Human settlement"@en;
        rdfs:comment "A resource/class defining a settlementofanysize"@en .


### Architecture ####
:Architecture rdf:type owl:Class ;
        rdfs:subClassOf dbo:ArchitecturalStructure  ;
        rdfs:label "Architektur"@de ,
                   "Architecture"@en ;
        rdfs:comment "A physical entity that was constructed by humans."@en .

### Accommodation ###
:Accommodation rdf:type owl:Class ;
        rdfs:subClassOf :Architecture ;
        rdfs:label "Unterkunft"@de ,
                   "Accommodation"@en ;
        rdfs:comment "A resource representing an accommodation."@en .


### Travel
:Travel rdf:type owl:Class ;
        rdfs:label "Reisen"@de ,
                "Travel"@en ;
        rdfs:comment "Any type of movement between two locations."@en .

### Journey  ###
:Journey rdf:type owl:Class ;
        rdfs:subClassOf :Travel ;
        owl:differentFrom :Excursion ;
        rdfs:label "Reise"@de ,
                "Journey"@en ;
        rdfs:comment "A journey from one place to another."@en .

### Excursion ###
:Excursion rdf:type owl:Class ;
        rdfs:subClassOf :Travel ;
        rdfs:label "Excursion"@de ,
                "Exkursion"@en ;
        rdfs:comment "An excursion from a location to another location and back."@en .

### Transportation ###
:Transportation rdf:type owl:Class ;
        rdfs:label "Transport"@de ,
                   "Transport"@en ;
        rdfs:comment "Represents a transportation event."@en .

### Mean Of Transportation ###
:MeanOfTransportation rdf:type owl:Class ;
        rdfs:label "Transportmittel"@de ,
                   "Mean of Transport"@en ;
        rdfs:comment "Represents a mean of transportation."@en .


### Stay ###
:Stay rdf:type owl:Class ;
        rdfs:label "Aufenthalt"@de,
                    "Stay"@en;
        rdfs:comment "A stay during a journey of undetermined amount of time"@en .


### LongTermStay ###
:LongTermStay rdf:type owl:Class ;
        rdfs:subClassOf :Stay ;
        rdfs:label "Langer Aufenthalt"@de ,
                   "Longterm Stay"@en ;
        rdfs:comment "The stay at the destination of a main journey."@en .

### ShortTermStay ###
:ShortTermStay rdf:type owl:Class ;
        rdfs:subClassOf :Stay ;
        owl:differentFrom :LongTermStay;
        rdfs:label "Kurzer Aufenthalt"@de ,
                   "Shortterm Stay"@en ;
        rdfs:comment "A shorterm stay at the destination of a main journey."@en .

### Manuscript ###
:Manuscript rdf:type owl:Class;
        rdfs:label "Manuskrip"@de ,
                "Manuscript"@en ;
        rdfs:comment "Represents a manuscript."@en .


### TravelDiary ###
:TravelDiary rdf:type owl:Class ;
        rdfs:subClassOf :Manuscript;
        rdfs:label "Reisetagebuch"@de ,
                "Travel Diary"@en ;
        rdfs:comment "Represents a travel diary."@en .


### TravelDiary ###
:Document rdf:type owl:Class ;
        rdfs:label "Dokument"@de ,
                "Document"@en ;
        rdfs:comment "Represents a written document, such as diary page."@en .

:TextContent rdf:type owl:Class ;
        rdfs:label "Text-Inhalt"@de ,
                "Text Content"@en ;
        rdfs:comment "Represents the text content of a document."@en .

#################################
### Properties
#################################
#TODO: fill in all labels
#TODO: fill in all comments


### hasParent
:hasParent rdf:type owl:ObjectProperty;
    rdfs:domain :Person ;
    rdfs:range :Person ;
    rdfs:label  "Hat Vater"@de,
                "Has parent"@en;
    rdfs:comment "A person who has parent."@en.

### isParentOf
:isParentOf rdf:type owl:ObjectProperty;
    owl:inverseOf :hasParent;
    rdfs:label  "Ist Vater"@de,
                "Is parent"@en;
    rdfs:comment "Someone who is parent of someone else."@en.

### participatedIn
:participatedIn rdf:type owl:ObjectProperty ;
    rdfs:domain :Person ;
    rdfs:range :Activity ;
    rdfs:label "Mitwirken"@de,
                "Participate"@en;
    rdfs:comment "Someone who participate in an activity."@en.

### hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
    owl:inverseOf :participatedIn ; # for inverse properties there is no need to define domain and range
    rdfs:label "hat ein Teilnehmer"@de,
                "Has participant"@en;
    rdfs:comment "Some activity which has someone as participant"@en.


### visited  #range is defined in shacl shapes, we leave it empty for RDFS validation
:visited rdf:type owl:ObjectProperty;
    rdfs:domain :Person ;
    rdfs:label "Besuchen"@de,
                "Visit"@en;
    rdfs:comment "physical presence"@en.

### hasSighted
:hasSighted rdf:type owl:ObjectProperty;  #range is defined in shacl shapes, we leave it empty for RDFS validation
    rdfs:domain :Person ;
    rdfs:label "schauen"@de,
                "Sight"@en;
    rdfs:comment "Short glance"@en.

### lived In
:livedInIn rdf:type owl:ObjectProperty;
     rdfs:domain :Person
     rdfs:range :Location ;
        rdfs:label "Leben"@de,
                   "Live"@en;
    rdfs:comment "Long residence in a place"@en.


## hasStop
:hasStop rdf:Type owl:ObjectProperty ; ### domain: rdf triple
        rdfs:range :Location ;
        rdfs:label "Haben hallstelle"@de,
                    "Has stop"@en;
        rdfs:comment "A Stop while on the journey from one place to another"@en.


### accordingTo ### domain: rdf triple
:accordingTo rdf:type owl:ObjectProperty ;
        rdfs:range :Person ; # we can add rdfs:domain rdf:Triple but since it is not release yet, we leave it out for now.
        rdfs:label "ist Informationsquelle"@de ,
                   "is source of information"@en ;
        rdfs:comment "Represents the person who is the source of information."@en .


### mentionedIn ### domain: rdf triple
:mentionedIn rdf:type owl:ObjectProperty ;
        rdfs:range :Document ;
        rdfs:label "erwähnt in Region"@de ,
                   "mentioned in region"@en ;
        rdfs:comment """Represent the document that is the source of the fact."""@en .



### hasAlternativeName
:hasAlternativeName rdf:type owl:DatatypeProperty ;
        rdfs:range xsd:string ;
        rdfs:label "Alternativer Name"@de ,
                   "Alternative name"@en ;
        rdfs:comment """Represents alternative name of a person or location."""@en .

### hasCost
:hasCost rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf currency:hasPrice ;
        rdfs:range currency:Price ;
        rdfs:label "Kosten"@de ,
                   "Cost"@en ;
        rdfs:comment """Represents the cost."""@en .

### hasCurrency
:hasCurrency rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf currency:hasCurrency ;
        rdfs:range currency:Currency ;
        rdfs:label "Währung"@de ,
                   "Currency"@en ;
        rdfs:comment """Represents the currency of a payment."""@en .

### hasDate
:hasDate rdf:type owl:DatatypeProperty ;
        rdfs:range xsd:date ;
        rdfs:label "Datum"@de ,
                   "Date"@en ;
        rdfs:comment """Represents the date, can be an interval."""@en .

### hasStartDate
:hasStartDate rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf xsd:hasDate ;
        rdfs:range xsd:date ;
        rdfs:label "Start-Datum"@de ,
                   "Start Date"@en ;
        rdfs:comment """Represents the start date"""@en .

### hasEndDate
:hasEndDate rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf xsd:hasDate ;
        rdfs:range xsd:date ;
        rdfs:label "End-Datum"@de ,
                   "End Date"@en ;
        rdfs:comment """Represents the end date"""@en .

### hasDestination
:hasDestination rdf:type owl:ObjectProperty ;
        rdfs:domain :Travel ;
        rdfs:range :Location ;
        rdfs:label "hat Reiseziel"@de ,
                   "has destination"@en ;
        rdfs:comment """Represents the destination of a one day excursion."""@en .

### hasStartLocation
:hasStartLocation rdf:type owl:ObjectProperty ;
        rdfs:domain :Travel ;
        rdfs:range :Location ;
        rdfs:label "Startort"@de ,
                   "Starting place of a trip"@en ;
        rdfs:comment """Represents the starting place of a journey."""@en .

### hasEncounter
:hasEncounter rdf:type owl:ObjectProperty ;
        rdfs:range :Person ;
        rdfs:label "Begegnung"@de ,
                   "Encounter"@en ;
        rdfs:comment """Represents an encounter with a person during a journey."""@en .


### hasExcursion
:hasExcursion rdf:type owl:ObjectProperty ;
        rdfs:range :Excursion ;
        rdfs:label "Ausflug"@de ,
                   "Excursion"@en ;
        rdfs:comment """Represents an excursion."""@en .

### hasSubExcursion
:hasSubExcursion rdf:type owl:ObjectProperty ;
        rdfs:range :Excursion ;
        rdfs:label "Exkursions-Teil"@de ,
                   "Sub excursion"@en ;
        rdfs:comment """Represents a part of an excursion."""@en .

### hasGeonameID
:hasGeonameID rdf:type owl:DatatypeProperty ;
        rdfs:domain :Location ;
        rdfs:range xsd:string ;
        rdfs:label "GeonameID"@de ,
                   "GeonameID"@en ;
        rdfs:comment """Represents the geoname ID of a location."""@en .

### has name xsd:string ##!! add class?
:hasName rdf:type owl:DatatypeProperty ;
        rdfs:domain :Location ;
        rdfs:range xsd:string ;
        owl:sameAs schema:name;
        rdfs:label "Name"@de ,
                   "Name"@en ;
        rdfs:comment """Represents the name of an entity."""@en .

