######################
## Documents / Text ##
######################

### Manuscript ###
:Manuscript rdf:type owl:Class;
        rdfs:label "Manuskript"@de ,
                "Manuscript"@en ;
        rdfs:comment "Represents a manuscript."@en .

### TravelDiary ### is diary etc  a :Document?
:TravelDiary rdf:type owl:Class ;
        rdfs:subClassOf :Manuscript;
        rdfs:label "Reisetagebuch"@de ,
                "Travel Diary"@en ;
        rdfs:comment "Represents a travel diary."@en .

### Document ### ######
:Document rdf:type owl:Class ;
        rdfs:label "Dokument"@de ,
                "Document"@en ;
        rdfs:comment "Represents a written document, such as diary page."@en .

### Text Content ########
:TextContent rdf:type owl:Class ;
        rdfs:label "Text-Inhalt"@de ,
                "Text Content"@en ;
        rdfs:comment "Represents the text content of a document."@en .

### Description ####
:Description rdf:type owl:Class ;
        owl:subClassOf :TextContent;
        rdfs:label "Beschreibung"@de ,
                "description"@en ;
        rdfs:comment "Represents an additional description."@en .


##################
### Predicates ###
##################

### hasStartLocation
:hasStartLocation rdf:type owl:ObjectProperty ;
        #rdfs:domain :Travel ; #rdf:triple
        rdfs:range :Location ;
        rdfs:label "Startort"@de ,
                   "has start location"@en ;
        rdfs:comment """Represents start location of a journey."""@en .

### hasDestination
:hasDestination rdf:type owl:ObjectProperty ;
        #rdfs:domain :Travel ;#rdf:triple
        rdfs:range :Location ; ## could it be sightseeing ? thus object?
        rdfs:label "hat Reiseziel"@de ,
                   "has destination"@en ;
        rdfs:comment "Represents the destination of a trip/journey."@en .

### hasArrival ###
:hasArrival rdf:type owl:DatatypeProperty;
        rdfs:label "hat Ankunft"@de ,
                   "has arrival"@en ;
        rdfs:comment "Represents the date and/or time of an arrival"@en .


### hasDeparture ### 
:hasDeparture rdf:type owl:DatatypeProperty;
        rdfs:label "hat Abfahrt"@de ,
                   "has departure"@en ;
        rdfs:comment "Represents the date and/or time of a departure."@en .


### hasStartDate ###
:hasStartDate rdf:type owl:DatatypeProperty;
        rdfs:label "Start"@de ,
                   "has start date"@en ;
        rdfs:comment "Represents the start an event"@en .

### hasEndDate ### what with activity
:hasEndDate rdf:type owl:DatatypeProperty;
        rdfs:label "Ende"@de ,
                   "has end date"@en ;
        rdfs:comment "Represents the end of a journey. In other words the arrival plus stay."@en .

### start ###
:start rdf:type owl:DatatypeProperty;
        rdfs:label "Start"@de ,
                   "start"@en ;
        rdfs:comment "Represents the start an event or activity"@en .

### end ### activity / incident? ###
:end rdf:type owl:DatatypeProperty;
        rdfs:label "Ende"@de ,
                   "end"@en ;
        rdfs:comment "Represents the end of a of an action or activity."@en .


### hasJourney ###
:hasJourney rdf:type owl:ObjectProperty ;
        #rdfs:domain :Person, rdf:triple ;
        rdfs:range :Journey ;
        rdfs:label "hat Reise"@de ,
                   "has journey"@en ;
        rdfs:comment """Represents (a journey) the main journey a person undertook."""@en .


### hasSubJourneyTo
:hasIntermediateDestination rdf:type owl:ObjectProperty ;
        #rdfs:domain :Travel; / rdf:Triple
        rdfs:range :Location ;
        rdfs:label "hat Zwischenstation"@de ,
                   "has intermediate destination"@en ;
        rdfs:comment """Represents an intermediate destination during a journey."""@en .


### hasSubJourney 
:hasStage rdf:type owl:ObjectProperty ;
        #rdfs:domain :Travel; rdf:Triple
        rdfs:range :Journey ;
        rdfs:label "hat Etappe"@de ,
                   "has stage"@en ;
        rdfs:comment """Represents a stage of a journey."""@en .

### activity predicates ###

# :sightingOf
:sightingOf rdf:type owl:ObjectProperty ; #owl:ObjectProperty
        rdfs:label "Besichtigung von"@de ,
                   "sighting of"@en ;
        rdfs:comment """Represents the target sighted during a sightseeing activity."""@en .

# :mealType
:mealType rdf:type owl:ObjectProperty ; #owl:ObjectProperty
        rdfs:label "Mahlzeit typ"@de ,
                   "meal type"@en ;
        rdfs:comment """Represents the type of meal during a dining activity."""@en .

# :cuisine
:cuisine rdf:type owl:ObjectProperty ; #owl:ObjectProperty
        rdfs:label "Küche"@de ,
                   "cuisine"@en ;
        rdfs:comment """Represents the type of cuisine of a dining activity."""@en .

# :entertainementType 
:entertainmentType rdf:type owl:ObjectProperty ; #owl:ObjectProperty
        rdfs:label "Unterhaltungsart"@de ,
                   "entertainement type"@en ;
        rdfs:comment """Represents the type of entertainment of an entertainment activity."""@en .
    
### hasStay
:hasStay rdf:type owl:ObjectProperty ;
        # rdfs:domain :Journey, rdf:triple;
        rdfs:range :Stay ;   
        rdfs:label "hat Aufenthalt."@de ,
                   "has Stay"@en ;
        rdfs:comment """Represents a Stay at a specific location during a Journey. Does not apply for excursion."""@en.

### hasAccommodation
:hasAccommodation rdf:type owl:ObjectProperty ;
        # rdfs:domain :Stay ;
        rdfs:range :Accommodation ;
        rdfs:label "hat Unterkunft"@de ,
                   "has Accommodation"@en ;
        rdfs:comment """Represents the accommodation of a stay."""@en .

### hasActivity
:hasActivity rdf:type owl:ObjectProperty ;
        rdfs:range :Activity;
        rdfs:label "hat Aktivität"@de ,
                   "has activity"@en ;
        rdfs:comment """Represents an activity during a trip."""@en .

### hasCost
:hasCost rdf:type owl:DatatypeProperty ;
        rdfs:subPropertyOf currencyA:hasPrice ;
        #rdfs:domain rdf:Triple;
        rdfs:range xsd:string ; #currency:Price
        rdfs:label "kostet"@de ,
                   "has cost"@en ;
        rdfs:comment """Represents a cost."""@en .

### has Currency
:hasCurrency rdf:type owl:ObjectProperty ;
        rdfs:label "Währung"@de ,
                   "Currency"@en ;
        rdfs:comment """Represents the currency of a payment."""@en .

### has transport
:hasTransport rdf:type owl:ObjectProperty ;
        #rdfs:domain :Travel ; #rdf:triple
        rdfs:range :MeanOfTransportion ;
        rdfs:label "hat Transport"@de ,
                   "has transport"@en ;
        rdfs:comment """Represents the transport used during a trip."""@en .

### has participant ###
:participatesIn rdf:type owl:ObjectProperty;
        rdfs:label "Nimmt Teil an"@de ,
                   "participates in"@en ;
        rdfs:comment """Represents a participant of a travel or activity"""@en .

### has participant ###
:hasParticipant rdf:type owl:ObjectProperty;
        owl:inverseOf :participatesIn;
        rdfs:range :Person ; 
        rdfs:label "Hat Partizipant:in"@de ,
                   "has Participant"@en ;
        rdfs:comment """Represents a participant of a travel or activity"""@en .

### hasDescription 
:hasDescription rdf:type owl:ObjectProperty;
        rdfs:range :TextContent ; 
        rdfs:label "hat Beschreibung"@de ,
                   "has description"@en ;
        rdfs:comment """Represents additional description"""@en .

### hasTextContent
:hasTextContent rdf:type owl:ObjectProperty;
        #rdfs:domain :Document ;
    rdfs:range :TextContent ;
    rdfs:label  "Text-Inhalt"@de,
                "Text Content"@en;
    rdfs:comment "Represents the text content of a document."@en.

### hasText
:hasText rdf:type owl:DatatypeProperty;
    rdfs:domain :TextContent ;
    rdfs:range xsd:string ;
    rdfs:label  "hat Text"@de,
                "has text"@en;
    rdfs:comment "Represents the actual text of a text content."@en.

### hasWikiLink
:hasWikiLink rdf:type owl:DatatypeProperty ;
        rdfs:range xsd:anyURI ;
        rdfs:label "hat link zu einer Wikidata Seite"@de ,
                   "has a link to a wikidata page"@en ;
        rdfs:comment """Represents a link to a wikidata page."""@en .

### has gnd 
:hasGnd rdf:type owl:DatatypeProperty ;
        rdfs:domain :Person ;
        rdfs:range xsd:string, xsd:anyURI ;
        rdfs:label "hat GND nummer"@de ,
                   "has gnd"@en ;
        rdfs:comment """Represents an identifier by the german national library."""@en .

### hasGeonameID
:hasGeonameID rdf:type owl:DatatypeProperty ;
        rdfs:domain :Location ;
        rdfs:range xsd:string ;
        rdfs:label "GeonameID"@de ,
                   "GeonameID"@en ;
        rdfs:comment """Represents the geoname ID of a location."""@en .


